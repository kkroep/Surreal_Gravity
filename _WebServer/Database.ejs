<!doctype html>
<head>
<title>Surreal Gravity</title>
<script src="http://code.jquery.com/jquery-1.11.1.js" type="text/javascript"></script>
<script src="client-app.js" type="text/javascript"></script>
</head>

<body>

<table class = "Games" border="0" style="width:100%">
  <tr id="firstrowGames">
    <td><h3>Wanneer</h3></td>
    <td><h3>Server</h3></td>
    <td><h3>Winnaar</h3></td>
    <td><h3>Finished</h3></td>
    <td><h3>Gamemode</h3></td>
    <td><h3>GAME_Id</h3></td>
    <% games_array.forEach(function(game) { %>
    <tr>
      <td><%=game.Wanneer%></td>
      <td><%=game.Server%></td>
      <td><%=game.Winnaar%></td>
      <td><%=game.Finished%></td>
      <td><%=game.Gamemode%></td>
      <td><%=game.GAME_Id%></td>
    </tr>
    <% }) %>
  </tr>          
</table>

<table class = "Participants" border="0" style="width:100%">
  <tr id="firstrowParticipants">
    <td><h3>PLAYER_Id</h3></td>
    <td><h3>GAME_Id</h3></td>
    <td><h3>PARTICIPANTS_Id</h3></td>
  </tr>          
</table>

<table class = "Players" border="0" style="width:100%">
  <tr id="firstrowPlayers">
    <td><h3>Naam</h3></td>
    <td><h3>Paswoord</h3></td>
    <td><h3>Gespeeld</h3></td>
    <td><h3>Gewonnen</h3></td>
    <td><h3>Wanneer</h3></td>
    <td><h3>PLAYER_Id</h3></td>
  </tr>

</table>


</body>
</html>
